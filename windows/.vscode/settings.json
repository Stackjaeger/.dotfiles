{
    "workbench.iconTheme": "material-icon-theme",
    "terminal.integrated.env.linux": {},
    "editor.guides.bracketPairs": true,
    "highlight.regexes": {
        "(\\$\\$\\$([a-zA-Z0-9_]+)\\$\\$\\$)": {
            "filterLanguageRegex": "vb", // Apply only if current file's language matches this regex. Requires double escaping
            "decorations": [
                {"color": "#c42828"}
            ]
        },
        "(\\{\\{([a-zA-Z0-9_]+)\\}\\})": {
            "filterLanguageRegex": "html", // Apply only if current file's language matches this regex. Requires double escaping
            "decorations": [
                {"color": "#c42828"}
            ]
        }
    },
    "git.autofetch": true,
    "workbench.colorTheme": "One Dark Pro Night Flat",
    "liveServer.settings.donotShowInfoMsg": true,
    "editor.wordWrap": "on",

    "material-icon-theme.files.associations": {
        "*.cdr": "corel-draw",
        "*.nc": "g-code",
        "*.gcode": "g-code"
    },
    "terminal.integrated.fontFamily": "'0xProto Nerd Font'",
    "editor.fontFamily": "'0xProto Nerd Font', monospace",
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.env.windows": {},
    "chat.commandCenter.enabled": false,
    "chat.notifyWindowOnConfirmation": false,
    "telemetry.feedback.enabled": false,
    "remote.SSH.experimental.chat": false,
    "chat.agent.enabled": false,
    "explorer.compactFolders": false,
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}